@prefix : <http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#> .

<http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5> rdf:type owl:Ontology ;
                                                                                            rdfs:comment "1. Title: Spanish Wine Checker Ontology" ,
                                                                                                         "2. Author: Daniel Pérez-Milá Sánchez" ,
                                                                                                         "3. Description: This ontology is about Spanish wines and their characteristics." ;
                                                                                            owl:versionInfo 1.0 .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#isComposedOf
:isComposedOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Wine ;
              rdfs:range :Grape ;
              rdfs:comment "A type of wine is made from one or more grape varieties." .


###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#isGrownIn
:isGrownIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Grape ;
           rdfs:range :Spanish_region ;
           rdfs:comment "A grape variety is grown in one or more Spanish regions." .


###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#isMadeIn
:isMadeIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Wine ;
          rdfs:range :Spanish_region ;
          rdfs:comment "A type of wine is produced in one or more Spanish regions." .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#hasAcidityLevel
:hasAcidityLevel rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Wine ;
                 rdfs:range xsd:int ;
                 rdfs:comment "A type of wine has a specific level of acidity when tasted within a range of 1 to 5." .


###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#hasAlcoholDegree
:hasAlcoholDegree rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Wine ;
                  rdfs:range xsd:float ;
                  rdfs:comment "A type of wine has a specific alcohol degree within a range of 0% to 100%." .


###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#hasBodyLevel
:hasBodyLevel rdf:type owl:DatatypeProperty ;
              rdfs:domain :Wine ;
              rdfs:range xsd:int ;
              rdfs:comment "A type of wine has a specific level of body when tasted within a range of 1 to 5." .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#Albarino_white_grape
:Albarino_white_grape rdf:type owl:Class ;
                      rdfs:subClassOf :White_grape ;
                      rdfs:comment "A type of white grape used to make a particular white or sparkling wine." .


###  http://www.semanticweb.org/danielperez-milasanchez/ontologies/2023/3/untitled-ontology-5#Albarino_wine
:Albarino_wine rdf:type owl:Class ;
               rdfs:subClassOf :White_wine ;
               rdfs:comment "Albariño wine (“alba-reen-yo”) is a delightfully refreshing coastal white that grows on the Iberian Peninsula. It’s loved for its rich stone fruit flavors, a hint of salinity, and zippy acidity." .


